{
    "breadcrumbs.enabled": false,
    "breadcrumbs.filePath": "off",
    "breadcrumbs.icons": false,
    "custom-ui-style.electron": {
        "frame": false
    },
    "custom-ui-style.external.imports": [
        "file:///home/toniogela/.config/nixos/vscodium/vscodium.js",
        "file:///home/toniogela/.config/nixos/vscodium/vscodium.css"
    ],
    "custom-ui-style.preferRestart": true,
    "custom-ui-style.reloadWithoutPrompting": true,
    "debug.console.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
    "debug.console.fontSize": 13,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.terminal.clearBeforeReusing": true,
    "debug.toolBarLocation": "docked",
    "diffEditor.codeLens": true,
    "diffEditor.renderSideBySide": false,
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.hideCursorInOverviewRuler": true,
    "editor.largeFileOptimizations": true,
    "editor.lineNumbers": "relative",
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.overviewRulerBorder": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        100
    ],
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.showFoldingControls": "always",
    "editor.stickyScroll.enabled": false,
    "editor.suggestSelection": "first",
    "editor.wordBreak": "keepAll",
    "errorLens.excludeBySource": [
        "bloop(6)"
    ],
    "errorLens.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
    "errorLens.gutterIconsEnabled": true,
    "explorer.confirmDelete": false,
    "explorer.excludeGitIgnore": true,
    "extensions.autoCheckUpdates": false,
    "extensions.ignoreRecommendations": false,
    "files.insertFinalNewline": false,
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "git.autofetch": true,
    "git.blame.editorDecoration.enabled": false,
    "git.blame.statusBarItem.enabled": false,
    "git.ignoreMissingGitWarning": true,
    "git.openRepositoryInParentFolders": "never",
    "indentRainbow.colorOnWhiteSpaceOnly": true,
    "indentRainbow.errorColor": "rgba(128;32;32;0)",
    "markdown.marp.breaks": "off",
    "markdown.marp.exportType": "html",
    "markdown.preview.typographer": true,
    "metals.autoImportBuilds": "all",
    "metals.bloopJvmProperties": [
        "-Xmx8G",
        "-Xms500M"
    ],
    "metals.enableIndentOnPaste": true,
    "metals.excludedPackages": [
        ".*javadsl.*",
        "akka.actor.typed.javadsl"
    ],
    "metals.serverProperties": [
        "-Xmx2G",
        "-Dmetals.enable-best-effort=true"
    ],
    "metals.superMethodLensesEnabled": false,
    "metals.verboseCompilation": true,
    "nix.enableLanguageServer": true,
    "nix.serverPath": "nixd",
    "nix.serverSettings": {
        "nixd": {
            "formatting": {
                "command": [
                    "nixfmt"
                ]
            },
            "options": {
                "home-manager": {
                    "expr": "(import ~/.config/home-manager/home.nix {}).personal.options"
                }
            }
        }
    },
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "scm.diffDecorationsGutterWidth": 2,
    "scm.diffDecorationsIgnoreTrimWhitespace": "inherit",
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.untrustedFiles": "open",
    "task.problemMatchers.neverPrompt": {
        "shell": true
    },
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.focusAfterRun": "terminal",
    "terminal.integrated.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.shellIntegration.showCommandGuide": false,
    "terminal.integrated.stickyScroll.enabled": false,
    "terminal.integrated.suggest.inlineSuggestion": "off",
    "terminal.integrated.suggest.showStatusBar": false,
    "terminal.integrated.suggest.suggestOnTriggerCharacters": false,
    "terminal.integrated.suggest.upArrowNavigatesHistory": false,
    "update.mode": "none",
    "vsicons.presets.hideExplorerArrows": true,
    "window.closeWhenEmpty": true,
    "window.commandCenter": false,
    "window.customTitleBarVisibility": "never",
    "window.titleBarStyle": "native",
    "window.zoomLevel": 1,
    "workbench.colorCustomizations": {
        "editorOverviewRuler.addedForeground": "#00000000",
        "editorOverviewRuler.deletedForeground": "#00000000",
        "editorOverviewRuler.modifiedForeground": "#00000000"
    },
    "workbench.colorTheme": "Nord",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 2,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.showTabs": "none",
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": "vscode-icons",
    "workbench.layoutControl.enabled": false,
    "workbench.preferredDarkColorTheme": "Nord",
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.tree.enableStickyScroll": false,
    "workbench.tree.indent": 4,
    "workbench.tree.renderIndentGuides": "none",
    "terminal.integrated.allowedLinkSchemes": [
        "file",
        "http",
        "https",
        "mailto",
        "vscode",
        "vscode-insiders",
        "vscodium"
    ],
    "[csv]": {
        "editor.inlayHints.maximumLength": 0
    },
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
    ],
    "rust-analyzer.completion.fullFunctionSignatures.enable": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.location": "hidden"
}